{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DWN Signed Object",
  "type": "object",
  "properties": {
    "payload": {
      "type": "string"
    },
    "signatures": {
      "type": "object",
      "properties": {
        "protected": {
          "type": "object",
          "alg": {
            "type": "string"
          },
          "kid": {
            "type": "string"
          }
        },
        "signature": {
          "type": "string"
        }
      },
      "required": ["protected", "signature"]
    }
  }
}
